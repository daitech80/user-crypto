<section id="sha256-hash">
    <h2>SHA-256 Hash</h2>
    <form action="/sha256/hash" method="post">
        <label for="sha-text">Text to Hash:</label>
        <input type="text" id="sha-text" name="text" required>
        <input type="submit" value="Hash">
    </form>
    <% if (cryptodata.hashedText) { %>
        <h3>Hashed Text:</h3>
        <p><%= cryptodata.hashedText %></p>
    <% } %>
</section>
